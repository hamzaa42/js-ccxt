// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tickers {
  ticker_name String   @id @unique
  circ_supply Float?
  tags        String[] // Assuming the tags are of type String. Change it to the appropriate type if needed.
}

model price_oi_data {
  ticker_name String
  timestamp   BigInt
  o           Float
  h           Float
  l           Float
  c           Float
  oi          Float?

  @@id(name:"stampTicker",[timestamp, ticker_name])
  @@index([timestamp, ticker_name(sort: Desc)])
}

model latest_insights {
  ticker_name String
  timestamp   BigInt
  mcap        Float
  mcap_oi     Float
  o           Float
  h           Float
  l           Float
  c           Float
  oi          Float

  r_p_15m     Float
  r_p_1h      Float
  r_p_4h      Float
  r_p_1D      Float
  r_p_1W      Float
  r_p_1M      Float

  r_oi_15m     Float
  r_oi_1h     Float
  r_oi_4h     Float
  r_oi_1D     Float
  r_oi_1W     Float?
  r_oi_1M     Float?

  r_MH        Float?
  r_WH        Float?
  r_DH        Float

  r_ML        Float?
  r_WL        Float?
  r_DL        Float

  o_p_15m     Float
  o_p_1h      Float
  o_p_4h      Float
  o_p_1D      Float
  o_p_1W      Float
  o_p_1M      Float

  o_oi_15m    Float?
  o_oi_1h     Float
  o_oi_4h     Float
  o_oi_1D     Float
  o_oi_1W     Float?
  o_oi_1M     Float?

  o_MH        Float?
  o_WH        Float?
  o_DH        Float
 
  o_ML        Float?
  o_WL        Float?
  o_DL        Float

  @@id([ticker_name, timestamp])
  @@index([ticker_name, timestamp(sort: Desc)])
  @@map(name: "latest_insights")
}
